      "stage": "dev",
      "versionFunctions": true,
      "compiledCloudFormationTemplate": {
        "AWSTemplateFormatVersion": "2010-09-09",
        "Description": "The AWS CloudFormation template for this Serverless application",
        "Resources": {
          "ServerlessDeploymentBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
              "BucketName": "bucket-server-less-11",
              "OwnershipControls": {
                "Rules": [
                  {
                    "ObjectOwnership": "BucketOwnerEnforced"
                  }
                ]
              },
              "BucketEncryption": {
                "ServerSideEncryptionConfiguration": [
                  {
                    "ServerSideEncryptionByDefault": {
                      "SSEAlgorithm": "AES256"
                    }
                  }
                ]
              }
            }
          },
          "ServerlessDeploymentBucketPolicy": {
            "Type": "AWS::S3::BucketPolicy",
            "Properties": {
              "Bucket": {
                "Ref": "ServerlessDeploymentBucket"
              },
              "PolicyDocument": {
                "Version": "2012-10-17",
                "Statement": [
                  {
                    "Sid": "DenyUnencryptedTransport",
                    "Effect": "Deny",
                    "Principal": "*",
                    "Action": "s3:*",
                    "Resource": [
                      {
                        "Fn::Join": [
                          "",
                          [
                            "arn:",
                            {
                              "Ref": "AWS::Partition"
                            },
                            ":s3:::",
                            {
                              "Ref": "ServerlessDeploymentBucket"
                            },
                            "/*"
                          ]
                        ]
                      },
                      {
                        "Fn::Join": [
                          "",
                          [
                            "arn:",
                            {
                              "Ref": "AWS::Partition"
                            },
                            ":s3:::",
                            {
                              "Ref": "ServerlessDeploymentBucket"
                            }
                          ]
                        ]
                      }
                    ],
                    "Condition": {
                      "Bool": {
                        "aws:SecureTransport": false
                      }
                    }
                  }
                ]
              }
            }
          },
          "AppLambdaFunction": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
              "Code": {
                "S3Bucket": "bucket-server-less-11",
                "S3Key": "serverless/foodway-backend/dev/foodway-backend.zip"
              },
              "Handler": "index.handler",
              "Runtime": "nodejs20.x",
              "FunctionName": "foodway-backend-dev-app",
              "MemorySize": 1024,
              "Timeout": 28,
              "Architectures": ["arm64"],
              "Environment": {
                "Variables": {
                  "MONGODB_URL": "mongodb+srv://foodway:1234@cluster0.urtrcl4.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0",
                  "CLIENT_URL": "http://bucket-server-less-11.s3-website-us-east-1.amazonaws.com",
                  "JWT_SECRET": "KJHGSDIOKJOHDF",
                  "EMAIL_PASS": "vqno zevg uxbn wylp",
                  "EMAIL_USER": "mandikishore2000@gmail.com",
                  "SMTP_HOST": "smtp.gmail.com",
                  "SMTP_PASS": "vqno zevg uxbn wylp",
                  "SMTP_PORT": "587",
                  "SMTP_USER": "mandikishore2000@gmail.com",
                  "AWS_S3_BUCKET_NAME": "bucket-server-less-11",
                  "EMAIL": "mandikishore2000@gmail.com",
                  "CLOUDINARY_CLOUD_NAME": "charan12",
                  "CLOUDINARY_API_KEY": "478681192216688",
                  "CLOUDINARY_API_SECRET": "du1JrEvTmjfmDiDa-Yi9cfP4MWc",
                  "RAZORPAY_KEY_ID": "rzp_test_RAcgtscfNdp1cg",
                  "RAZORPAY_KEY_SECRET": "MWn5u9N8yFYjgQpVlNvix93f",
                  "FRONTEND_URL": "http://bucket-server-less-11.s3-website-us-east-1.amazonaws.com",
                  "IS_LOCAL": "false"
                }
              },
              "Role": {
                "Fn::GetAtt": ["IamRoleLambdaExecution", "Arn"]
              }
            }
          }
        }
      }
